{
    "name": "k-tab",
    "version": "1.0",
    "manifest_version": 3,
    "description": "chrome new tab",
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' http://localhost:3001; object-src 'self'"
    },
    "permissions": [
        "bookmarks",
        "storage"
    ]
}
